<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <Preference
        android:key="PACKAGE_VERSION_NAME"
        android:title="@string/version_label"
        android:icon="@null"
        android:summary="1.0.0">
    </Preference>
    <Preference
        android:title="@string/website_label"
        android:icon="@null"
        android:summary="http://www.belugamobile.com">
        <intent android:action="android.intent.action.VIEW"
            android:data="http://www.belugamobile.com" />
    </Preference>
    <Preference
        android:title="@string/developer_label"
        android:icon="@null"
        android:summary="Feng Hu">
        <intent android:action="android.intent.action.VIEW"
            android:data="http://hufeng.info" />
    </Preference>
    <Preference
        android:key="TRANSLATION_CONTRIBUTION"
        android:title="@string/translation_label"
        android:icon="@null"
        android:summary="@string/translation_summary">
    </Preference>
</PreferenceScreen>
