<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <TextView
        android:id="@+id/safe_category_picture"
        android:drawableLeft="@drawable/category_icon_picture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/safe_category_image"
        />
    <TextView
        android:id="@+id/safe_category_video"
        android:drawableLeft="@drawable/category_icon_video"
       	android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/safe_category_video"
        />

</LinearLayout>
